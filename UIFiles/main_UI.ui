/*****************QSpinBox, QDoubleSpinBox*******************/

QSpinBox, QDoubleSpinBox
{
	background-color: transparent;
	border-bottom: 2px solid #D7D7D9;
	color:#fff;
	border-radius: None;
	font-size: 16px;
	min-height: 25px;
	min-width: 70px;
	qproperty-alignment: AlignCenter;
}

QSpinBox:disabled ,
QDoubleSpinBox:disabled
{
	border-bottom: 2px solid #F0F0F2;
	color: rgb(120, 120, 120);
}

QSpinBox::up-arrow, 
QDoubleSpinBox::up-arrow
{   
	image: url(:/icons/icons/icons8-plus-36.png);
	width: 16px;
	height: 16px;
}

QSpinBox::down-arrow,  
QDoubleSpinBox::down-arrow
{   
	image: url(:/icons/icons/icons8-minus-36.png);
	width: 16px;
	height: 16px;
}

QSpinBox::up-arrow:hover, 
QDoubleSpinBox::up-arrow:hover
{   
	image: url(:/icons/icons/icons8-plus-36-hover.png);
	width: 16px;
	height: 16px;
}

QSpinBox::down-arrow:hover,  
QDoubleSpinBox::down-arrow:hover
{   
	image: url(:/icons/icons/icons8-minus-36-hover.png);
	width: 16px;
	height: 16px;
}



QSpinBox::up-arrow:disabled, 
QDoubleSpinBox::up-arrow:disabled
{   
	image: url(:/icons/icons/plus_icon_gray.png);
	width: 16px;
	height: 16px;
}

QSpinBox::down-arrow:disabled ,  
QDoubleSpinBox::down-arrow:disabled
{   
	image: url(:/icons/icons/minus_icon_gray.png);
	width: 16px;
	height: 16px;
}

QSpinBox::up-button,
QDoubleSpinBox::up-button
{
	border:none;
    min-width:30px;
    min-height: 29px;
    subcontrol-origin: margin;
    subcontrol-position: right;
    top: 0px;
    right: 0px;
}

QSpinBox::down-button,
QDoubleSpinBox::down-button
{
    min-width:30px;
    min-height: 29px;
    subcontrol-origin: margin;
    subcontrol-position: left;
    top: 0px;
    right: 0px;
}

QSpinBox::up-button,
QSpinBox::down-button,
QDoubleSpinBox::up-button,
QDoubleSpinBox::down-button
{
	background-color: transparent;
}

QSpinBox::up-button:disabled ,
QSpinBox::down-button:disabled ,
QDoubleSpinBox::up-button:disabled ,
QDoubleSpinBox::down-button:disabled
{
    subcontrol-origin: border;
}

QSpinBox:focus, QDoubleSpinBox:focus
{
	border-bottom: 2px solid #7892DF;
}


